
	<div class="col-xs-3">
		<h4>Feeds</h4>
		<table class="table"><td></table>
		<ul>
			<li>Top Stories</li>
			<br>
			<% 2.times do %>
				<% x = @questions.sample %>
				<li><a href="/questions/<%=x.id%>">
				<%= x.title %></a></li>
			<% end %>
			<a href="/questions"><h4>See More...</h4></a>
		</ul>
	</div>

	<div class="col-xs-7">
	<a href="/questions"><h4>Questions</h4></a>
	<table class="table"><td></table>
	<% 5.times do %>
		<% x = @questions.sample %>
			<h4><a href="/questions/<%=x.id%>">
				<%= x.title %></a></h4>
				<h4><span class="label label-info"><%= x.votecount %></span>
				<span><small><a href="/questions/<%=x.user%>">
				<%= x.user.name %></a></small></span></h4>
				<p><%=x.body%></p>
	<% end %>
	<a href="/questions"><h4>See More...</h4></a>

	<br>


	<a href="/users"><h2>Users</h2></a>
		<% @users.each do |x| %>
			<h5><small><a href="/users/<%=x.id%>">
				<%= x.name %> <%= x.email %></a></small></h5>
	<% end %>
